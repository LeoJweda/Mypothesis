#panel-container
  .panel.ui-dialog.ui-widget.ui-widget-content.ui-corner-all
    .panel-header.ui-widget-header.ui-corner-all
      = title
      
    .panel-content.ui-dialog-content.ui-widget-content
      = yield
    - if buttons != ""
      .panel-buttonpane.ui-dialog-buttonpane.ui-widget-content
        = buttons